        "core-js": "=3.15.2",
        "dayjs": "=1.10.5",
        "jquery": "=3.6.0",
        "portal-vue": "^2.1.7",
        "v-calendar": "^2.3.0",
        "vue": "^2.6.12",
        "vue-chartjs": "^3.5.1",
        "vue-class-component": "^7.2.3",
        "vue-click-outside": "^1.1.0",
