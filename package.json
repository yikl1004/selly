{
    "name": "selly",
    "version": "0.1.0",
    "private": false,
    "scripts": {
        "dev": "vue-cli-service serve --mode development",
        "prd": "vue-cli-service serve --mode production",
        "serve:client": "vue-cli-service serve",
        "serve:server": "ts-node --project ./tsconfig.base.json ./server/index.ts",
        "story": "start-storybook -p 6006 -c .storybook watch-css -s ./src",
        "build": "vue-cli-service build --mode production",
        "lint": "vue-cli-service lint",
        "build-storybook": "build-storybook",
        "chromatic": "npx chromatic --project-token e38d1f04d1f6 --auto-accept-changes",
        "postinstall": "husky install",
        "tsc-staged": "ts-node --project ./tsconfig.base.json ./cli/tsc-staged.ts $PWD",
        "vue-staged": "ts-node --project ./tsconfig.base.json ./cli/vue-staged.ts",
        "test": "ts-node --project ./tsconfig.base.json ./test.ts"
    },
    "dependencies": {
        "animate.css": "=4.1.1",
        "ant-design-vue": "=1.7.5",
        "axios": "=0.21.1",
        "axios-extensions": "=3.1.3",
