        "vue-type-check": "^1.1.0"
    },
    "babel": {
        "presets": [
            "@vue/cli-plugin-babel/preset"
        ]
    },
    "browserslist": {
        "production": [
            "> 1%",
            "last 2 versions",
            "not dead"
        ],
        "development": [
            "> 1%",
            "last 2 versions",
            "not dead"
        ]
    },
    "engines": {
        "node": "14"
    },
    "lint-staged": {
        "*.{ts,vue}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.ts": "npm run tsc-staged",
        "*.vue": "npm run vue-staged"
    }
