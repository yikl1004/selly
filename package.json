{
    "name": "loca",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "concurrently \"npm run serve:server\" \"npm run serve:client\"",
        "serve:client": "vue-cli-service serve",
        "serve:server": "ts-node --project ./tsconfig.base.json ./server/index.ts",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "tsc-staged": "ts-node --project ./tsconfig.base.json ./cli/tsc-staged.ts $PWD",
        "vue-staged": "ts-node --project ./tsconfig.base.json ./cli/vue-staged.ts",
        "postinstall": "husky install",
        "story": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.35",
        "@fortawesome/free-solid-svg-icons": "^5.15.3",
        "@fortawesome/vue-fontawesome": "^2.0.2",
        "ant-design-vue": "^1.7.4",
        "axios": "^0.21.1",
        "axios-extensions": "^3.1.3",
        "chart.js": "^3.1.0",
        "core-js": "^3.8.3",
        "vue": "^2.6.11",
        "vue-chartjs": "^3.5.1",
        "vue-class-component": "^7.2.3",
        "vue-fragment": "^1.5.2",
        "vue-image-upload-resize": "^2.3.0",
        "vue-property-decorator": "^9.1.2",
        "vue-router": "^3.5.1",
        "vuex": "^3.6.2",
        "vuex-module-decorators": "^1.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.13.14",
        "@storybook/addon-actions": "^6.2.1",
        "@storybook/addon-essentials": "^6.2.1",
        "@storybook/addon-links": "^6.2.1",
        "@storybook/vue": "^6.2.1",
        "@types/json-server": "^0.14.3",
        "@types/shelljs": "^0.8.8",
        "@typescript-eslint/eslint-plugin": "^4.20.0",
        "@typescript-eslint/parser": "^4.20.0",
        "@vue/cli-plugin-babel": "^4.5.11",
        "@vue/cli-plugin-eslint": "^4.5.11",
        "@vue/cli-plugin-router": "^4.5.11",
        "@vue/cli-plugin-typescript": "^4.5.11",
        "@vue/cli-plugin-vuex": "^4.5.11",
        "@vue/cli-service": "^4.5.11",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/eslint-config-typescript": "^5.0.2",
        "babel-loader": "^8.2.2",
        "chalk": "^4.1.0",
        "concurrently": "^5.3.0",
        "dotenv": "^8.2.0",
        "eslint": "^6.7.2",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^7.1.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-vue": "^6.2.2",
        "husky": "^5.0.8",
        "json-server": "^0.16.3",
        "lint-staged": "^10.5.3",
        "node-sass": "^4.12.0",
        "prettier": "^1.19.1",
        "reflect-metadata": "^0.1.13",
        "sass-loader": "^8.0.2",
        "shelljs": "^0.8.4",
        "ts-node": "^9.1.1",
        "tsconfig-paths-webpack-plugin": "^3.5.1",
        "typescript": "^4.1.3",
        "vue-loader": "^15.9.6",
        "vue-template-compiler": "^2.6.12",
        "vue-type-check": "^1.1.0"
    },
    "engines": {
        "node": "14"
    },
    "lint-staged": {
        "*.{ts,vue}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.ts": "npm run tsc-staged",
        "*.vue": "npm run vue-staged"
    },
    "browserslist": {
        "production": [
            "> 1%",
            "last 2 versions",
            "not dead"
        ],
        "development": [
            "> 1%",
            "last 2 versions",
            "not dead"
        ]
    },
    "babel": {
        "presets": [
            "@vue/cli-plugin-babel/preset"
        ]
    }
}
