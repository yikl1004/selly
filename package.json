        "@vue/eslint-config-typescript": "^7.0.0",
        "babel-loader": "^8.2.2",
        "chalk": "^4.1.1",
        "chalk-web": "^1.0.0",
        "chromatic": "^5.9.2",
        "dotenv": "^8.2.0",
        "eslint": "^7.26.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^7.1.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-vue": "^7.14.0",
        "husky": "^5.2.0",
